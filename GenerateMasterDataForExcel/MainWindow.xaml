<Window x:Class="GenerateMasterDataForExcel.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:GenerateMasterDataForExcel"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <ComboBox x:Name="EnemyTypeComboBox" HorizontalAlignment="Left" Margin="10,75,0,0" VerticalAlignment="Top" Width="120"></ComboBox>
        <TextBox x:Name="EnemyNameTextField" HorizontalAlignment="Left" Height="23" Margin="135,76,0,0" TextWrapping="Wrap" Text="" VerticalAlignment="Top" Width="189" TextChanged="GetChangedText" Grid.ColumnSpan="3"/>
        <ComboBox x:Name="EnemyParamComboBoxFirst" HorizontalAlignment="Left" Margin="329,75,0,0" VerticalAlignment="Top" Width="120"/>
        <ComboBox x:Name="EnemyParamComboBoxLast" HorizontalAlignment="Left" Margin="454,76,0,0" VerticalAlignment="Top" Width="120"/>
        <TextBlock x:Name="TextField" HorizontalAlignment="Left" Height="120" Margin="10,120,0,0" TextWrapping="Wrap" Text="" VerticalAlignment="Top" Width="314" Grid.ColumnSpan="3">
        </TextBlock>
    </Grid>
</Window>
